{
  "name": "fox-css",
  "version": "0.8.0",
  "description": "quickly style semantic html without the hassle",
  "scripts": {
    "serve": "gulp --require esm serve",
    "build": "gulp --require esm build",
    "postversion": "pnpm run build && git push origin dev && ./deploy.sh && git push --tags && pnpm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eankeen/fox-css.git"
  },
  "keywords": [
    "css",
    "semantic",
    "fox",
    "simple",
    "minimal",
    "style",
    "modern"
  ],
  "author": "Edwin Kofler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eankeen/fox-css/issues"
  },
  "homepage": "https://github.com/eankeen/fox-css#readme",
  "engines": {
    "node": ">=12.10"
  },
  "main": "dist/fox.min.css",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "modern-normalize": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "postcss-simple-vars": "^5.0.2",
    "postcss-strip-inline-comments": "^0.1.5"
  }
}
